UAAC,GAAD,EAAQ;AACX,UAAA,MAAM,CAAC,GAAD,CAAN;AACH,SAVD;AAYH,OAbI,CAAP;AAcF;AAjBO,GARI;AA2BP,EAAA,OA3BO,qBA2BG;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBAEO,KAAI,CAAC,QAAL,EAFP;;AAAA;AAEZ,cAAA,KAAI,CAAC,KAFO;AAAA;AAAA;;AAAA;AAAA;AAAA;AAIZ,cAAA,KAAI,CAAC,KAAL,GAAa,YAAI,OAAjB;;AAJY;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMhB;AAjCa,CAAf","sourcesContent":["<template>\n  <div class=\"hello\">\n    <h1> Loads </h1>\n    <p class=\"error\" v-if=\"error\"> {{ error }} </p>\n      <div class=\"loads-container wrapper\" align=\"center\">\n        <div class=\"load\"\n          v-for=\"(load, index) in loads\"\n          v-bind:item=\"load\"\n          v-bind:index=\"index\"\n          v-bind:key=\"load._id\"\n        >\n          <!-- <p class=\"text\"> {{ load.name }} </p> -->\n        <div class=\"deck\" align=\"center\">\n            <div class=\"card\" style=\"width: 18rem;\">\n            <img :src=\"load.image\" class=\"card-img-top\" />\n              <div class=\"card-body\">\n                <h5 class=\"card-title\"> {{ load.name }} </h5>\n                <h6 class=\"card-subtitle mb-2 text-muted\"> P {{load.price }}.00 </h6>\n                <p class=\"card-text\">Can be used to avail promos! </p>\n                <!-- <a href=\"/checkout\" class=\"card-link\">Card link</a> -->\n                <!-- <router-link to=\"/checkout\">Buy</router-link> -->\n                <router-link :to=\"`/checkout/${load._id}`\"> Buy </router-link>\n              </div>\n            </div>\n            <br>\n        </div>\n    \n        </div>\n      </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  name: 'Load',\n  data() {\n    return {\n      loads: [],\n      error: ''\n    }\n  },\n  methods: {\n    getLoads() {\n      const url = 'http://192.168.254.117:3000/'\n      return new Promise ((resolve,reject) => {\n            axios.get(url).then((res) => {\n                const data = res.data;\n                resolve(\n                    data.map(load => ({\n                        ...load,\n                    }))\n                );\n            })\n            .catch((err)=> {\n                reject(err);\n            })\n            \n        });\n    }\n  },\n  async created() {\n    try {\n      this.loads = await this.getLoads()\n    } catch(err) {\n      this.error = err.message\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n  .wrapper {\n    display: inline-flex;\n    max-width: 700px;\n    flex-wrap: wrap;\n    padding-top: 12px;\n    align-items: center;\n    justify-content: center;\n  }\n\n  .card {\n    box-shadow: rgba(0, 0, 0, 0.117647) 0px 1px 6px, rgba(0, 0, 0, 0.117647) 0px 1px 4px;\n    max-width: 200px;\n    margin: 12px;\n    transition: .15s all ease-in-out;\n  }\n</style>\n"],"sourceRoot":""}]}